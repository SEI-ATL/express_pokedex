<div class="container">
    <h1 class="text-center">Favorite Pokemon</h1>

    <% pokemon.forEach(function(pokemon) { %>
        <div class="well">
            <h2>
                <%= pokemon.name %>
            </h2>
            <a href="/pokemon/<%= pokemon.name %>" class="btn btn-info">View details</a>
        </div>
    <% }); %>
</div>