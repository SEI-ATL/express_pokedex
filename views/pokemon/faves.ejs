<h1>Your favorite Pokemon:</h1>

<form action="/faves" method="GET">
    <ul>
        <% allPoke.forEach(fave => { %>
            <a href="/pokemon/<%= fave.name.toLowerCase() %> ">
                <li>
                    <%= fave.name %> 
                </li>  
            </a>
        <% }) %> 
    </ul> 
</form>
